meshFile = meshes/mesh
folderVTK = ./vtk/

numberFractures = 3


[mediumData]

        [./domain]

                meshExternal = none
                meshFolder = ./meshes/

                spatialDiscretization = 21
                spatialInclination = 0.
                lengthAbscissa = 1.
                lengthOrdinate = 1.
                lengthQuota = 1.
                meshType = GT_PK(2,1)
                spaceDimension = 2.

        [../]

        [./darcy]

                invK = 1.
                invKDist11 = 1.
                invKDist12 = 0.
                invKDist22 = 1.

        [../]

[../]


[fractureData0]

spaceDimension = 1.

        [./levelSet]
		
				levelSet = y+x-1.19
				ylevelSet = y-0.41+0.22*t
				xlevelSet = x-0.78-0.22*t          
				levelSetCut = -1
				yMap = 0.41-0.22*t
				xMap = 0.78+0.22*t
				jacMap = [1./sqrt(2)]
				normalMap = [1.,1.]
		
		        integrationTypeSimplex = IM_STRUCTURED_COMPOSITE(IM_TRIANGLE(3),1)

        [../]

        [./domain]

		        thickness = 0.013
		
		        spacing = x+0.78
		
		        spatialDiscretization = 81
		        translateAbscissa = 0.78
		        lengthAbscissa = 0.22 #0.5
		        lengthOrdinate = 0.
		        lengthQuota = 0.
		        meshType = GT_PK(1,1)
		
		
		        integrationTypeVelocity = IM_GAUSS1D(3)
		        integrationTypePressure = IM_GAUSS1D(2)
		
		        FEMTypeVelocity = FEM_PK(1,1)
		        FEMTypePressure = FEM_PK(1,0)
		        FEMTypeLinear = FEM_PK(1,1)

        [../]

        [./darcy]

                etaNormal = 0.01
                etaNormalDistribution = 1. #1.+1000.*(x>0.25)-1000.*(x>0.75)
                etaTangential = 100.
                etaTangentialDistribution = 1.

                source = 0.0
                solution = -1.
                velocity = 0.

        [../]


[../]


